@using System.Diagnostics.CodeAnalysis
@using Google.Protobuf.WellKnownTypes

@Timestamp.ToDateTimeOffset().ToLocalTime().ToString(Format)

@code {
    [Parameter, EditorRequired]
    public required Timestamp Timestamp { get; set; }

    [Parameter] [StringSyntax("DateTimeFormat")] public string? Format { get; set; } = "dd.MM.yyyy HH:mm:ss";
}